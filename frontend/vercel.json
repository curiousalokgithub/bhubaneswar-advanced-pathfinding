{
  "version": 2,
  "name": "bhubaneswar-pathfinding-frontend",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_API_URL": "https://bhubaneswar-pathfinding-api.onrender.com/api",
    "VITE_BACKEND_URL": "https://bhubaneswar-pathfinding-api.onrender.com",
    "VITE_APP_NAME": "Bhubaneswar Pathfinding",
    "VITE_APP_VERSION": "2.0.0",
    "VITE_DEFAULT_LAT": "20.2961",
    "VITE_DEFAULT_LNG": "85.8245",
    "VITE_DEFAULT_ZOOM": "13"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://bhubaneswar-pathfinding-api.onrender.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

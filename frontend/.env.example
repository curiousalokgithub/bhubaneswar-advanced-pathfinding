# Frontend Environment Variables
# Copy this file to .env.local and update with your values

# Google Maps API Key (required for all map functionalities)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_BASE_URL=http://localhost:5000

# Application Settings
VITE_APP_NAME=Bhubaneswar Pathfinding
VITE_APP_VERSION=2.0.0

# Map Configuration  
VITE_DEFAULT_LAT=20.2961
VITE_DEFAULT_LNG=85.8245
VITE_DEFAULT_ZOOM=12

# Features Toggle
VITE_ENABLE_REAL_TIME_ROUTES=true
VITE_ENABLE_TRAVEL_PLANS=true
VITE_ENABLE_PHOTO_GALLERY=true
VITE_MAP_CENTER_LAT=20.2961
VITE_MAP_CENTER_LNG=85.8245
VITE_DEFAULT_ZOOM=12

# Optional: Map Provider (if using custom tiles)
# VITE_MAP_PROVIDER_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png
# VITE_MAP_ATTRIBUTION=&copy; OpenStreetMap contributors

# Environment Variables Template for Bhubaneswar Pathfinding API
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=5000
NODE_ENV=production

# CORS Configuration
FRONTEND_URL=https://your-frontend-domain.com
ALLOWED_ORIGINS=https://your-frontend-domain.com,http://localhost:3000,http://localhost:5173,http://localhost:3005

# Google Maps API Configuration
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable: Places API, Directions API, Distance Matrix API, Geocoding API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Configuration
API_VERSION=2.0.0
API_TITLE=Bhubaneswar Advanced Pathfinding API

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL=300

# Features Configuration
ENABLE_GOOGLE_MAPS=true
ENABLE_TRAVEL_PLANS=true
ENABLE_REAL_TIME_ROUTES=true

# Application Info
APP_NAME=Bhubaneswar Advanced Pathfinding System
APP_VERSION=2.0.0
